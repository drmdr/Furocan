NEXT_PUBLIC_APP_TAGLINE=
NEXT_PUBLIC_APP_OG_TITLE=
NEXT_PUBLIC_APP_OG_DESCRIPTION=
NEXT_PUBLIC_APP_OG_IMAGE=

# Farcaster Account Association (npx create-onchain --manifest)
FARCASTER_HEADER=
FARCASTER_PAYLOAD=
